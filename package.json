{
  "name": "github-api",
  "version": "1.3.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "commit": "yarn lint-staged && git-cz",
    "lint": "eslint ./src ./tests",
    "build": "babel src docs --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "tsnd -r dotenv/config -r tsconfig-paths/register --inspect --transpile-only --exit-child --ignore-watch node_modules src/config/http/server.ts",
    "start": "node -r dotenv/config ./dist/config/http/server.js",
    "test": "yarn vitest --config ./config/vitest/default.ts",
    "test:staged": "yarn test --changed --watch=false",
    "test:external": "yarn vitest --config ./config/vitest/external.ts",
    "test:coverage": "yarn vitest run --coverage --config ./config/vitest/coverage.ts",
    "docs:generate": "yarn vitest --config ./config/vitest/docsReporter.ts"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.29.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "fs-extra": "^11.1.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "vitest-mock-extended": "^1.1.0"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "yamljs": "^0.3.0",
    "zod": "^3.22.4"
  }
}
